{
  "data_dir": "./data",
  "pipelines": [
    {
      "name": "sistemas",
      "repo": "SistemaRepository",
      "csv": "sistema_nitrogenos.csv",
      "delimiter": ",",
      "encoding": "utf-8",
      "mappings": {
        "sistema": [
          "codigo",
          "nombre"
        ]
      }
    },
    {
      "name": "plantas",
      "repo": "PlantaRepository",
      "csv": "plantas.csv",
      "delimiter": ";",
      "encoding": "utf-8",
      "defaults": {
        "sistema_id": 1
      },
      "mappings": {
        "planta_id": "codigo",
        "nombre": "nombre"
      },
      "fks": {
        "sis_nitrogeno_id": {
          "table": "sistema",
          "repo": "SistemaRepository",
          "field": "codigo",
          "target_field": "sistema_id"
        }
      }
    },
    {
      "name": "area",
      "repo": "AreaRepository",
      "csv": "areas.csv",
      "delimiter": ";",
      "encoding": "latin-1",
      "defaults": {
        "planta_id": 5
      },
      "mappings": {
        "Código": "codigo",
        "Centro Operativo": "nombre"
      },
      "fks": {
        "Ubicación": {
          "table": "planta",
          "repo": "PlantaRepository",
          "field": "codigo",
          "target_field": "planta_id"
        }
      }
    },
    {
      "name": "operarios",
      "repo": "OperarioRepository",
      "csv": "operarios.csv",
      "delimiter": ";",
      "encoding": "utf-8",
      "unique_field": "codigo_empleado",
      "mappings": {
        "operarioID": "codigo_empleado",
        "nombreOperario": "nombre"
      }
    },
    {
      "name": "medios_preparados",
      "repo": "MedioPreparadoRepository",
      "csv": "medios_reactivos.csv",
      "delimiter": ";",
      "encoding": "utf-8",
      "mappings": {
        "MEDIO": "codigo",
        "Identificación": "nombre"
      }
    }
  ]
}